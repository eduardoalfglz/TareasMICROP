                                        ;movb #88,BIN2          
21da 27 37                              beq returnPH
21dc 1d 10 00 08                        bclr BANDERAS2,$08      
21e0 c1 1a                              cmpb #26                ;26 es el numero minimo de ticks si es menor el resultado de la velocidad es mayor a 255 y se desborda           
